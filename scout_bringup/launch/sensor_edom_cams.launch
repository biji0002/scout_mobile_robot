<launch>
  <!-- Set this to your camera's name -->
  <arg name="cam_name" value="edom_cam0" />

  <!-- Start the GSCAM node -->
  <env name="GSCAM_CONFIG" value="v4l2src device=/dev/video0 ! video/x-raw,width=1280,height=720,format=UYVY,framerate=30/1 ! videoconvert ! video/x-raw,format=BGR ! appsink" />
  <node pkg="gscam" type="gscam" name="$(arg cam_name)">
    <param name="camera_name" value="$(arg cam_name)" />
    <!-- <param name="camera_info_url" value="package://localcam/calibrations/${NAME}.yaml" /> -->
    <remap from="camera/image_raw" to="$(arg cam_name)/image_raw" />
  </node>

  <!-- View the raw and rectified output -->
  <node pkg="image_view" type="image_view" name="raw_view" >
    <remap from="image" to="/$(arg cam_name)/image_raw" />
  </node>
</launch>
